{
  "name": "funkcia",
  "version": "0.0.1",
  "description": "",
  "author": "Luke Morales <lukemorales@live.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemorales/funkcia.git"
  },
  "bugs": {
    "url": "https://github.com/lukemorales/funkcia/issues"
  },
  "keywords": [],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs"
    },
    "./exceptions": {
      "default": "./dist/exceptions.mjs",
      "import": "./dist/exceptions.mjs",
      "require": "./dist/exceptions.js",
      "types": "./dist/exceptions.d.ts",
      "module": "./dist/exceptions.mjs"
    },
    "./functions": {
      "default": "./dist/functions.mjs",
      "import": "./dist/functions.mjs",
      "require": "./dist/functions.js",
      "types": "./dist/functions.d.ts",
      "module": "./dist/functions.mjs"
    },
    "./json": {
      "default": "./dist/json.mjs",
      "import": "./dist/json.mjs",
      "require": "./dist/json.js",
      "types": "./dist/json.d.ts",
      "module": "./dist/json.mjs"
    },
    "./option": {
      "default": "./dist/option.mjs",
      "import": "./dist/option.mjs",
      "require": "./dist/option.js",
      "types": "./dist/option.d.ts",
      "module": "./dist/option.mjs"
    },
    "./predicate": {
      "default": "./dist/predicate.mjs",
      "import": "./dist/predicate.mjs",
      "require": "./dist/predicate.js",
      "types": "./dist/predicate.d.ts",
      "module": "./dist/predicate.mjs"
    },
    "./result": {
      "default": "./dist/result.mjs",
      "import": "./dist/result.mjs",
      "require": "./dist/result.js",
      "types": "./dist/result.d.ts",
      "module": "./dist/result.mjs"
    },
    "./url": {
      "default": "./dist/url.mjs",
      "import": "./dist/url.mjs",
      "require": "./dist/url.js",
      "types": "./dist/url.d.ts",
      "module": "./dist/url.mjs"
    },
    "./uri": {
      "default": "./dist/uri.mjs",
      "import": "./dist/uri.mjs",
      "require": "./dist/uri.js",
      "types": "./dist/uri.d.ts",
      "module": "./dist/uri.mjs"
    }
  },
  "prettier": "@lukemorales/prettier-config",
  "eslintConfig": {
    "extends": [
      "lukemorales",
      "lukemorales/oss"
    ],
    "rules": {
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/member-ordering": "off",
      "@typescript-eslint/strict-boolean-expressions": "off"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:tsup": "tsup --dts --minify",
    "changeset": "changeset",
    "clean": "rimraf dist",
    "dev": "pnpm run test:coverage --ui",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src --ext .ts",
    "lint:eslint:fix": "pnpm run lint:eslint --fix",
    "lint:format": "prettier --check ./src",
    "lint:format:fix": "prettier --write ./src",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "prerelease": "run-p build test:ci",
    "release": "changeset publish",
    "test": "vitest --passWithNoTests",
    "test:ci": "pnpm run test:coverage --run",
    "test:coverage": "pnpm run test --coverage"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@lukemorales/prettier-config": "^1.1.1",
    "@types/node": "^22.4.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.56.0",
    "eslint-config-lukemorales": "^0.5.0",
    "jest-extended": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.2.4",
    "typescript": "4",
    "vitest": "^2.0.5"
  }
}
